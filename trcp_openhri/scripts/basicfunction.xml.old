<?xml version="1.0" encoding="UTF-8" ?>
<grammar xmlns="http://www.w3.org/2001/06/grammar"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.w3.org/2001/06/grammar
                             http://www.w3.org/TR/speech-grammar/grammar.xsd"
         xml:lang="jp"
         version="1.0" mode="voice" root="command">
  <lexicon uri="basicfunction-lex-jp.xml"/>
  <rule id="command">
    <one-of>
      <item>にほん</item>
      <item>ふらんす</item>
      <item>せかい</item>
      <item>ぽぱい</item>
      <item>うし</item>
    </one-of>
    <one-of>
      <item>の</item>
      <item>で</item>
      <item>と</item>
    </one-of>
    <one-of>
      <item>しゅと</item>
      <item>いちばんたかいやま</item>
      <item>いちばんひろいみずうみ</item>
      <item>いちばんながいかわ</item>
      <item>いちばんおおきなくに</item>
      <item>いちばんおもいどうぶつ</item>
      <item>がーるふれんど</item>
      <item>ちゅうごく</item>
      <item>あし</item>
      <item>た</item> <!-- 雑音を無視するための無意味な単語 -->
      <item>つ</item> <!-- 雑音を無視するための無意味な単語 -->
    </one-of>
    <item>は</item>
    <one-of>
      <item> どこですか</item>
      <item> どこ</item>
      <item> なに</item>
      <item> なんですか</item>
      <item> だれですか</item>
      <item> だれ</item>
      <item> どちらがおおきいですか</item>
      <item> どちらがおおきい</item>
      <item> なんぼんですか</item>
      <item> なんぼん</item>
      <item>つ</item> <!-- 雑音を無視するための無意味な単語 -->
      <item>た</item> <!-- 雑音を無視するための無意味な単語 -->
    </one-of>

  </rule>
</grammar>

